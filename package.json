    "tsconfig-paths": "^4.1.1",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml,yaml,html}": [
      "prettier --write"
    ]
  }
