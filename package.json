{
  "name": "serverless-event-engine",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "nx": "nx",
    "build": "nx build",
    "serve": "nx run-many --target=serve --all",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "deploy": "nx run-many --target=deploy --all",
    "affected": "nx affected",
    "format": "nx format:write",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "test:affected": "nx affected:test --base=origin/main --head=HEAD --exclude=infrastructure",
    "deploy:affected": "nx affected --target=deploy --all",
    "lint:affected": "nx affected:lint --base=origin/main --head=HEAD",
    "g:service": " npx nx workspace-generator service",
    "g:aggregate": " npx nx workspace-generator aggregate",
    "g:command": " npx nx workspace-generator command",
    "g:event": " npx nx workspace-generator event",
    "g:consume-event": " npx nx workspace-generator consume-event",
    "g:produce-event": " npx nx workspace-generator produce-event",
    "g:query": " npx nx workspace-generator query",
    "g:vo": " npx nx workspace-generator vo",
    "g:process-manager": " npx nx workspace-generator process-manager",
    "cody": "ts-node tools/codyserver/index.ts"
  },
  "dependencies": {
    "@middy/core": "2.5.7",
    "@middy/http-json-body-parser": "2.5.7",
    "ajv": "^8.11.2",
    "ajv-formats": "^2.1.1",
    "jsonwebtoken": "8.5.1",
    "jwk-to-pem": "^2.0.5",
    "middy": "0.36.0",
    "pg": "^8.8.0",
    "pg-cursor": "^2.7.4",
    "tslib": "2.0.0",
    "ulid": "2.3.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "3.55.0",
    "@jest/transform": "27.5.1",
    "@nrwl/cli": "14.1.2",
    "@nrwl/devkit": "14.1.2",
    "@nrwl/eslint-plugin-nx": "14.1.2",
    "@nrwl/jest": "14.1.2",
    "@nrwl/linter": "14.1.2",
    "@nrwl/node": "14.1.2",
    "@nrwl/nx-cloud": "14.0.3",
    "@nrwl/tao": "14.1.2",
    "@nrwl/workspace": "14.1.2",
    "@proophboard/cody-server": "^0.3.4",
    "@proophboard/cody-types": "^0.3.2",
    "@proophboard/cody-utils": "^0.2.0",
    "@proophboard/schema-to-typescript": "^0.2.0",
    "@types/aws-lambda": "8.10.93",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/node": "^16.18.11",
    "@types/node-fetch": "2.6",
    "@types/pg": "^8.6.6",
    "@types/pg-cursor": "^2.7.0",
    "@types/serverless": "3.0.2",
    "@types/terser-webpack-plugin": "5.0.4",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.18.0",
    "esbuild": "0.14.36",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "json-schema-to-ts": "^2.6.2",
    "lint-staged": "12.3.8",
    "node-fetch": "2.6",
    "prettier": "2.6.2",
    "serverless": "3.18.1",
    "serverless-esbuild": "1.27.1",
